<%= form_for @chore do |f| %>
<%= f.label :name %>: <%= f.text_field :name %> <br>
<%= f.label :cycle %>: <%= f.select :cycle, options_for_select(["Daily", "Weekly", "Monthly", "Seasonally", "Yearly"],["86400", "604800", "2592000", "7776000"]), include_blank: true%> <br>
<%= f.label :notes %>: <%= f.text_area :notes %> <br>
<%= f.submit %>
<% end %>
